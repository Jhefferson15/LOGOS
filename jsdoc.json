{
    "source": {
        "include": [
            "Frontend/js"
        ],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "opts": {
        "template": "node_modules/clean-jsdoc-theme",
        "encoding": "utf8",
        "destination": "docs/",
        "recurse": true,
        "verbose": true,
        "theme_opts": {
            "default_theme": "dark",
            "search": true,
            "title": "LOGOS Documentation",
            "menu": [
                {
                    "title": "Website",
                    "link": "../index.html",
                    "target": "_blank",
                    "class": "menu-item",
                    "id": "website_link"
                }
            ]
        }
    }
}