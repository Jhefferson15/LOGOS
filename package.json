{
  "name": "logos-documentation",
  "version": "1.0.0",
  "description": "Documentation generation for LOGOS project",
  "scripts": {
    "docs:build": "jsdoc -c jsdoc.json && node scripts/enhance-docs.js",
    "docs:watch": "onchange \"Frontend/js/**/*.js\" -- npm run docs:build"
  },
  "devDependencies": {
    "clean-jsdoc-theme": "^4.3.0",
    "docdash": "^2.0.2",
    "jsdoc": "^4.0.2",
    "jsdoc-to-markdown": "^9.1.3",
    "onchange": "^7.1.0",
    "sharp": "^0.34.5"
  }
}
